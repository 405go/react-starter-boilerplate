  "scripts": {
    "gulp-dev": "gulp watch",
    "gulp-publish": "gulp --type production",
    "eslint": "eslint ./src",
    "stylelint": "stylelint \"src/components/**/*.css\" \"src/pages/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "validate": "npm ls",
    "test": "mocha --compilers js:babel-register",
